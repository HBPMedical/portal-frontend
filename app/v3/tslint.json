{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**",
      "**/*.ts",
      "*.json",
      "**/*.json",
      "**/*.test.ts*",
      "**/__mocks__/**"
    ]
  },
  "rules": {
    "no-console": false
  }
}
