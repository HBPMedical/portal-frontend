    "codegen": "graphql-codegen -r dotenv/config --config codegen.yml ",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix",
      "pre-push": "yarn lint:fix"
